<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.EmployeeManageProfile">
    <children>
        <Label layoutX="300.0" layoutY="7.0" text="Manage Profile">
            <font>
                <Font size="30.0" />
            </font>
        </Label>
        <TextField fx:id="firstName" layoutX="149.0" layoutY="66.0" prefHeight="39.0" prefWidth="206.0" />
        <Label layoutX="39.0" layoutY="72.0" text="First Name" />
        <TextField fx:id="lastName" layoutX="549.0" layoutY="66.0" prefHeight="39.0" prefWidth="206.0" />
        <Label layoutX="413.0" layoutY="72.0" text="Last Name" />
        <Label layoutX="41.0" layoutY="135.0" text="Username" />
        <TextField fx:id="initialEmail" layoutX="112.0" layoutY="327.0" prefHeight="39.0" prefWidth="206.0" />
        <Label layoutX="46.0" layoutY="338.0" text="Email" />
        <Button fx:id="initialAddButton" layoutX="398.0" layoutY="356.0" mnemonicParsing="false" onAction="#addEmail" prefHeight="39.0" prefWidth="129.0" text="Add Email" />
        <Button layoutX="284.0" layoutY="536.0" mnemonicParsing="false" onAction="#updateAccount" text="Update" />
        <Button layoutX="435.0" layoutY="536.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
        <Label fx:id="errorMessage" layoutX="61.0" layoutY="570.0" textFill="#d31e1e">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <ScrollPane layoutX="112.0" layoutY="375.0" prefHeight="137.0" prefWidth="251.0">
            <content>
                <Pane fx:id="addEmailPane" />
            </content>
        </ScrollPane>
        <Button layoutX="398.0" layoutY="416.0" mnemonicParsing="false" onAction="#removeEmail" prefHeight="39.0" prefWidth="142.0" text="Remove Email" />
        <Label layoutX="414.0" layoutY="135.0" text="Site Name" />
        <Label layoutX="424.0" layoutY="197.0" text="Phone" />
        <Label layoutX="44.0" layoutY="268.0" text="Address" />
        <TextField fx:id="phone" layoutX="549.0" layoutY="182.0" prefHeight="39.0" prefWidth="206.0" />
      <Label layoutX="41.0" layoutY="197.0" text="Employee ID" />
      <Label fx:id="username" layoutX="153.0" layoutY="130.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="siteName" layoutX="555.0" layoutY="130.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="employeeID" layoutX="153.0" layoutY="192.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="address" layoutX="153.0" layoutY="262.0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <CheckBox fx:id="visitorAccount" layoutX="627.0" layoutY="376.0" mnemonicParsing="false" text="Visitor Account?" />
    </children>
</AnchorPane>
